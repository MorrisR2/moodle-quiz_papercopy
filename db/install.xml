<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="mod/quiz/report/papercopy/db" VERSION="20111225" COMMENT="XMLDB file for Moodle mod/quiz/report/papercopy"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="papercopy" COMMENT="Default comment for papercopy, please edit me" NEXT="paper_batches">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
    <TABLE NAME="paper_batches" COMMENT="List of batch-created paper copies of quizzes." PREVIOUS="papercopy">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" NEXT="quiz"/>
        <FIELD NAME="quiz" TYPE="int" LENGTH="4" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" COMMENT="The quiz from which this batch was created." PREVIOUS="id" NEXT="usages"/>
        <FIELD NAME="usages" TYPE="text" LENGTH="medium" NOTNULL="false" SEQUENCE="false" COMMENT="comma-separated list of usages created by this batch" PREVIOUS="quiz" NEXT="delivery_method"/>
        <FIELD NAME="delivery_method" TYPE="int" LENGTH="2" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" COMMENT="integer representation of a delivery method mode" PREVIOUS="usages" NEXT="include_key"/>
        <FIELD NAME="include_key" TYPE="int" LENGTH="1" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" COMMENT="non-zero if a key should be included with the printout" PREVIOUS="delivery_method"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>